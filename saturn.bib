	pages = {55--64},
	file = {Fotios et al. - 2019 - A whole-year approach showing that ambient light l.pdf:/home/eugeni/Zotero/storage/3PRQPTUD/Fotios et al. - 2019 - A whole-year approach showing that ambient light l.pdf:application/pdf},
}

