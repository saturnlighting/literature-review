	pages = {86--94},
	file = {SAGE PDF Full Text:/home/eugeni/Zotero/storage/6EUE5LB6/Nabavi Niaki et al. - 2016 - Road Lighting Effects on Bicycle and Pedestrian Ac.pdf:application/pdf},
}

