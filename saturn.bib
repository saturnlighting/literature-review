	author = {Asgarzadeh, Morteza and Fischer, Dorothee and Verma, Santosh K. and Courtney, Theodore K. and Christiani, David C.},
	year = {2018},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/ajim.22849},
	keywords = {bicycle, environment, crash, injury severity, light},
